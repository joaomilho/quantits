{
  "name": "@quantits/physics",
  "version": "1.0.0",
  "description": "Physics units and dimensions for QuantiTS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@quantits/core": "^1.0.0"
  },
  "keywords": [
    "units",
    "physics",
    "SI",
    "metric",
    "imperial",
    "measurement",
    "type-safe",
    "typescript"
  ]
}

