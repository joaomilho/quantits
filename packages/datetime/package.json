{
  "name": "@quantits/datetime",
  "version": "1.0.0",
  "description": "Date and time units for QuantiTS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@quantits/physics": "^1.0.0"
  },
  "keywords": [
    "units",
    "datetime",
    "time",
    "date",
    "calendar",
    "type-safe",
    "typescript"
  ]
}

