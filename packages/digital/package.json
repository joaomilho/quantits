{
  "name": "@quantits/digital",
  "version": "1.0.0",
  "description": "Digital/information units and dimensions for QuantiTS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@quantits/core": "^1.0.0",
    "@quantits/physics": "^1.0.0"
  },
  "keywords": [
    "units",
    "digital",
    "information",
    "bytes",
    "bits",
    "bandwidth",
    "type-safe",
    "typescript"
  ]
}

