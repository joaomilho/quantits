{
  "name": "@quantits/digital",
  "version": "0.0.1",
  "description": "Digital/information units and dimensions for QuantiTS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quantits/quantits.git",
    "directory": "packages/digital"
  },
  "homepage": "https://github.com/quantits/quantits#readme",
  "bugs": {
    "url": "https://github.com/quantits/quantits/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@quantits/core": "^0.0.1",
    "@quantits/physics": "^0.0.1"
  },
  "keywords": [
    "units",
    "digital",
    "information",
    "bytes",
    "bits",
    "bandwidth",
    "type-safe",
    "typescript"
  ]
}
