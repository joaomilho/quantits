{
  "name": "@quantits/time",
  "version": "0.0.1",
  "description": "Time units and conversions for QuantiTS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quantits/quantits.git",
    "directory": "packages/time"
  },
  "homepage": "https://github.com/quantits/quantits#readme",
  "bugs": {
    "url": "https://github.com/quantits/quantits/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@quantits/physics": "^0.0.1",
    "luxon": "^3.5.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2"
  },
  "keywords": [
    "units",
    "time",
    "duration",
    "luxon",
    "iso8601",
    "type-safe",
    "typescript"
  ]
}
